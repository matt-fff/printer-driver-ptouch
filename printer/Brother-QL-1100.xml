<printer id="printer/Brother-QL-1100">
  <make>Brother</make>
  <model>QL-1100</model>
  <pcmodel>BRQ1100</pcmodel>
  <mechanism>
    <thermal/>
    <!--not "color"-->
    <resolution>
      <dpi>
        <x>300</x>
        <y>600</y>
      </dpi>
    </resolution>
  </mechanism>
  <url>https://www.brother.co.uk/labelling/ql-printers/ql-1100</url>
  <lang>
    <proprietary />
  </lang>
  <autodetect>
    <general>
      <ieee1284>MFG:Brother;CMD:PT-CBP;MDL:QL-1100;CLS:PRINTER;</ieee1284>
      <commandset>PT-CBP</commandset>
      <description>Brother QL-1100</description>
      <manufacturer>Brother</manufacturer>
      <model>QL-1100</model>
    </general>
  </autodetect>
  <functionality>B</functionality>
  <driver>ptouch-ql</driver>
  <unverified />
  <!--no "contrib_url"-->
  <comments>
    <en>
    This driver does not support two-color printing.
    </en>
  </comments>
  <select>
    <option id="opt/Brother-PTQL-TransferMode" />
    <option id="opt/Brother-QL-StatusNotification" />
    <option id="opt/Brother-PTQL-Resolution">
      <arg_defval>ev/300dpi</arg_defval>
      <enum_val id="ev/300dpi" />
      <enum_val id="ev/300x600dpi" />
    </option>
    <option id="opt/Brother-PTQL-BytesPerLine">
      <enum_val id="ev/162" />
    </option>
    <option id="opt/Brother-PTQL-PixelTransfer">
      <enum_val id="ev/RLE" sense="false" />
      <enum_val id="ev/ULP" />
    </option>
    <option id="opt/Brother-PTQL-AutoCut" />
    <option id="opt/Brother-PTQL-CutLabel" />
    <option id="opt/Brother-QL-PageSize">
      <arg_defval>ev/102x152</arg_defval>
      <enum_val id="ev/102x152" />
      <enum_val id="ev/102x51" />
      <enum_val id="ev/102mm" />
    </option>
  </select>
</printer>
